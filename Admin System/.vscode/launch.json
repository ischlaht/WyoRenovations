    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome",
            "url": "http://localhost/Companytemplate/newwyocon/index/index.php",
            "webRoot": "${C:/wamp64/www/Companytemplate/NewWyoCon}"
        },
        {
            "name": "Listen for XDebug",
            "type": "php",
            "request": "launch",
            "port": 3600
        },
        {
            "name": "Launch currently open script",
            "type": "php",
            "request": "launch",
            "program": "${C:/'wamp64/'bin/'php/'php7.1.9/php.exe}",
            "cwd": "${fileDirname}",
            "port": 3600
        }
    ]
}